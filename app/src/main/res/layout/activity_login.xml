        <ImageView
            android:id="@+id/login_imageView_num4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num3"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/gon128white" />

        <ImageView
            android:id="@+id/login_imageView_num5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num4"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/inuyasha128white" />

        <ImageView
            android:id="@+id/login_imageView_num6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="36dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/login_imageView_num0"
            app:srcCompat="@drawable/kiriko128white" />

        <ImageView
            android:id="@+id/login_imageView_num7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num6"
            app:layout_constraintTop_toBottomOf="@+id/login_imageView_num1"
            app:srcCompat="@drawable/lucy128white" />

        <ImageView
            android:id="@+id/login_imageView_num8"
            android:layout_width="50dp"
