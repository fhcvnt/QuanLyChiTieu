        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="85dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sharingan256" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/login_constraintlayout_grouppass"
        android:layout_width="330dp"
        android:layout_height="170dp"
        android:layout_marginTop="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/login_imageView_num0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/aries128white" />

        <ImageView
            android:id="@+id/login_imageView_num1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num0"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/chopper128white" />

        <ImageView
            android:id="@+id/login_imageView_num2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num1"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/erza128white" />

        <ImageView
            android:id="@+id/login_imageView_num3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num2"
            app:layout_constraintTop_toTopOf="parent"
