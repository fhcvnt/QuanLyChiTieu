            app:layout_constraintTop_toBottomOf="@+id/login_imageView_num5"
            app:srcCompat="@drawable/natsu128white" />

        <ImageView
            android:id="@+id/login_imageView_num11"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="62dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/login_imageView_num6"
            app:srcCompat="@drawable/orihime128white" />

        <ImageView
            android:id="@+id/login_imageView_num12"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num11"
