            app:srcCompat="@drawable/saitama128white" />

        <ImageView
            android:id="@+id/login_imageView_num13"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num12"
            app:layout_constraintTop_toBottomOf="@+id/login_imageView_num8"
            app:srcCompat="@drawable/songoku128white" />

        <ImageView
            android:id="@+id/login_imageView_num14"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/login_imageView_num13"
            app:layout_constraintTop_toBottomOf="@+id/login_imageView_num9"
            app:srcCompat="@drawable/yukino128white" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/login_textView_password"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:text="***"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/login_constraintlayout_grouppass"
        app:layout_constraintEnd_toStartOf="@+id/login_imageView_backspace"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/login_imageView_backspace"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/login_constraintlayout_grouppass"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/backspace64" />

    <ImageView
        android:id="@+id/login_imageView_login"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_constraintlayout_grouppass"
        app:srcCompat="@drawable/login64" />

    <ImageView
        android:id="@+id/login_imageView_hinhdaidien"
